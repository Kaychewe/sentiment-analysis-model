	Tokenization -> "Stopword Removal"
	"Stopword Removal" -> "Stemming/Lemmatization"
	"Stemming/Lemmatization" -> "POS Tagging"
	"POS Tagging" -> Chunking
	Chunking -> "Named Entity Recognition"
	"Named Entity Recognition" -> "Sentiment Analysis"
}
